get_filename_component(SELF_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
if(EXISTS "${SELF_DIR}/LassPythonBuildExports.cmake")
	# we're in the build tree, use the build tree exports
	include("${SELF_DIR}/LassPythonBuildExports.cmake")
	set(LassPython_INCLUDE_DIRS "@CMAKE_CURRENT_SOURCE_DIR@" "@config_dir@" "@PYTHON_INCLUDE_PATH@")
else()
	include("${SELF_DIR}/LassPythonExports.cmake")
	get_filename_component(LassPython_INCLUDE_DIRS "${SELF_DIR}/../include" ABSOLUTE)
endif()
set(LassPython_LIBRARIES lass)

set(LassPython_MAJOR_VERSION @LASS_MAJOR_VERSION@)
set(LassPython_MINOR_VERSION @LASS_MINOR_VERSION@)
set(LassPython_PATCHLEVEL @LASS_PATCHLEVEL@)

set(LassPython_PYTHON_LIBRARY "@PYTHON_LIBRARY@")
set(LassPython_PYTHON_DEBUG_LIBRARY "@PYTHON_DEBUG_LIBRARY@")
set(LassPython_PYTHON_EXECUTABLE "@PYTHON_EXECUTABLE@")
