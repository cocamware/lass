get_filename_component(_lass_self_dir "${CMAKE_CURRENT_LIST_FILE}" PATH)
if(EXISTS "${_lass_self_dir}/LassBuildExports.cmake")
	# we're in the build tree, use the build tree exports
	include("${_lass_self_dir}/LassBuildExports.cmake")
else()
	include("${_lass_self_dir}/LassExports.cmake")
endif()

set(Lass_MAJOR_VERSION @Lass_VERSION_MAJOR@)
set(Lass_MINOR_VERSION @Lass_VERSION_MINOR@)
set(Lass_PATCHLEVEL @Lass_VERSION_PATCH@)

# deprecated
if(EXISTS "${_lass_self_dir}/LassBuildExports.cmake")
	set(Lass_INCLUDE_DIRS "$<TARGET_PROPERTY:lass,INCLUDE_DIRECTORIES>")
else()
	get_filename_component(Lass_INCLUDE_DIRS "${_lass_self_dir}/../include" ABSOLUTE)
endif()
set(Lass_LIBRARIES Lass::lass)
if(TARGET Lass::lass_python)
	list(APPEND Lass_LIBRARIES Lass::lass_python)
endif()
set(Lass_PYTHON_LIBRARY "@PYTHON_LIBRARY@")
set(Lass_PYTHON_DEBUG_LIBRARY "@PYTHON_DEBUG_LIBRARY@")
set(Lass_PYTHON_EXECUTABLE "@PYTHON_EXECUTABLE@")
set(Lass_BUILD_WITHOUT_ITERATOR_DEBUGGING @BUILD_WITHOUT_ITERATOR_DEBUGGING@)
set(Lass_EXTRA_CXX_FLAGS "$<$<BOOL:$<TARGET_PROPERTY:lass,INTERFACE_COMPILE_OPTIONS>>:$<JOIN:$<TARGET_PROPERTY:lass,INTERFACE_COMPILE_OPTIONS>, >>")
