get_filename_component(SELF_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
if(EXISTS "${SELF_DIR}/LassBuildExports.cmake")
	# we're in the build tree, use the build tree exports
	include("${SELF_DIR}/LassBuildExports.cmake")
	set(Lass_INCLUDE_DIRS "@CMAKE_CURRENT_SOURCE_DIR@" "@config_dir@" "@PYTHON_INCLUDE_PATH@")
else()
	include("${SELF_DIR}/LassExports.cmake")
	get_filename_component(Lass_INCLUDE_DIRS "${SELF_DIR}/../include" ABSOLUTE)
endif()
set(Lass_LIBRARIES lass)

set(Lass_PYTHON_LIBRARY "@PYTHON_LIBRARY@")
set(Lass_PYTHON_DEBUG_LIBRARY "@PYTHON_DEBUG_LIBRARY@")
set(Lass_PYTHON_EXECUTABLE "@PYTHON_EXECUTABLE@")
